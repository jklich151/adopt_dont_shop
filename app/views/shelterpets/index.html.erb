<center>
<h1> Our Pets </h1>
<%= @shelter.name %>
<% @shelter.pets.each do |pet| %>
  <p><%= image_tag(pet.image, size: "400x300") %></p>
  <p><%= pet.name %></p>
  <p><%= pet.age %></p>
  <p><%= pet.sex %></p>
<% end %>
<%= link_to 'Create Pet', "/shelters/#{@shelter.id}/pets/new", method: :get %>
</center>
